{
    "name": "cron-lambda",
    "version": "0.0.0",
    "description": "Deployable worker that requests image generation",
    "main": "index.js",
    "type": "module",
    "scripts": {
        "test": "pnpm vitest",
        "test:run": "pnpm vitest run",
        "build": "pnpm tsc -b",
        "bundle": "pnpm build && pnpm esbuild dist/index.js --bundle --platform=node --target=node22 --outfile=bundle/index.mjs --external:aws-sdk --format=esm --tree-shaking=true",
        "deploy": "pnpm bundle && pnpm node ./scripts/deploy.mjs",
        "destroy": "pnpm node ./scripts/destroy.mjs",
        "clean": "rm -rf ./dist && rm -rf ./bundle && rm tsconfig.tsbuildinfo"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "packageManager": "pnpm@10.12.1",
    "dependencies": {
        "utils": "workspace:^",
        "exponential-backoff": "^3.1.3",
        "openai": "^6.7.0",
        "@aws-sdk/client-s3": "^3.922.0",
        "@aws-sdk/client-ssm": "^3.910.0"
    },
    "devDependencies": {
        "esbuild": "^0.25.10",
        "@types/node": "^24.7.2"
    }
}
