{
  "name": "cron-lambda",
  "version": "0.0.0",
  "description": "Deployable worker that requests image generation",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "pnpm tsc -b",
    "bundle": "pnpm build && pnpm esbuild src/index.ts --bundle --platform=node --target=node22 --outfile=bundle/index.js --external:aws-sdk",
    "deploy": "pnpm bundle && pnpm node ./scripts/deploy.mjs",
    "destroy": "pnpm node ./scripts/destroy.mjs"
  },
